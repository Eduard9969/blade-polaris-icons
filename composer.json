{
    "name": "eduard9969/blade-polaris-icons",
    "description": "A package to easily make use of Polaris Icons in your Laravel Blade views.",
    "keywords": ["Blade", "Shopify", "Polaris", "Laravel"],
    "homepage": "https://github.com/Eduard9969/blade-polaris-icons",
    "license": "MIT",
    "authors": [
        {
            "name": "Eduard Samoilenko",
            "homepage": "https://readytest.tk/"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2",
        "blade-ui-kit/blade-icons": "^1.1",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "codeat3/blade-icon-generation-helpers": "^0.4",
        "codeat3/phpcs-styles": "^1.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Eduard9969\\BladePolarisIcons\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eduard9969\\BladePolarisIcons\\BladePolarisIconsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "generate": "vendor/bin/blade-icons-generate",
        "format": "vendor/bin/php-cs-fixer fix"
    }
}
